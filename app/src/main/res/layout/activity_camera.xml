<?xml version = "1.0" encoding = "utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android = "http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools = "http://schemas.android.com/tools"
	android:layout_width = "match_parent"
	android:layout_height = "match_parent"
	android:background="@drawable/sampa_background1"
	>

	<Button
		android:id="@+id/botaoLer"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/botao"
		android:text="Load photo"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toLeftOf="@+id/botaoCamera"
		app:layout_constraintTop_toTopOf="parent" />


	<Button
		android:id="@+id/botaoCamera"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/botao"
		android:text="Take photo"
		app:layout_constraintLeft_toRightOf="@+id/botaoLer"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<ImageView
		android:id="@+id/imageFaceCrop"
		android:layout_width="200dp"
		android:layout_height="200dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toStartOf="@+id/imageMatchDataset"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.674" />

	<ImageView
		android:id="@+id/imageMatchDataset"
		android:layout_width="200dp"
		android:layout_height="200dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="1.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.674"
		tools:srcCompat="@tools:sample/avatars" />

	<ImageView
		android:id="@+id/imageFrame"
		android:layout_width="411dp"
		android:layout_height="273dp"
		android:layout_marginTop="72dp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:srcCompat="@tools:sample/backgrounds/scenic" />

	<Button
		android:id="@+id/botaoHistorico"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Historico"
		app:layout_constraintBottom_toBottomOf="parent"
		android:background="@drawable/botao"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.977"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.847" />

	<Button
		android:id="@+id/botaoPerfil"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Perfil"
		android:background="@drawable/botao"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.978"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.954" />

	<TextView
		android:id="@+id/textViewNameOfSuspectMain"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginEnd="224dp"
		android:layout_marginRight="224dp"
		android:text="Nome: "
		android:textColor="#000000"
		android:textSize="24sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintHorizontal_bias="1.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.828" />

	<TextView
		android:id="@+id/textViewAccuracyMain"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="40dp"
		android:layout_marginBottom="44dp"
		android:text="AcurÃ¡cia:"
		android:textColor="#040303"
		android:textSize="24sp"
		android:typeface="normal"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintHorizontal_bias="0.041"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintVertical_bias="0.02" />

	<Button
		android:id="@+id/formula"
		android:layout_width="79dp"
		android:layout_height="50dp"
		android:layout_marginTop="16dp"
		android:text="Button"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="332dp" />
</androidx.constraintlayout.widget.ConstraintLayout>
